# Portas de acesso
OLLAMA_PORT=11434
WEBUI_PORT=3000

# Configurações do Ollama
OLLAMA_HOST=0.0.0.0
OLLAMA_ORIGINS=*

# Limitação de recursos
MEMORY_LIMIT=16G        # Máximo de RAM que pode usar
MEMORY_RESERVE=4G       # RAM mínima reservada

# Modelos para instalar automaticamente (separados por vírgula)
DEFAULT_MODELS=deepseek-v3,qwen2.5-coder:7b

# Configurações de rede (IP do servidor)
# Deixe comentado para usar todas as interfaces
# SERVER_IP=192.168.0.150

# Configurações de log
COMPOSE_LOG_LEVEL=INFO